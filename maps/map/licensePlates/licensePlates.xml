                <color shaderParameter="colorMat0" color="0 0.033 0.319"/>
            </variation>
            <variation> <!-- fantasy license plate -->
                <value node="0|0" posX="-0.172" numerical="false" alphabetical="true" special="true"/>
                <value node="0|1" posX="-0.109" numerical="false" alphabetical="true" special="true"/>
                <value node="0|2" posX="-0.048" numerical="true" alphabetical="false" special="true"/>
