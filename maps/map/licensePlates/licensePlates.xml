                <color shaderParameter="colorMat0" color="0 0.033 0.319"/>
            </variation>
        </variations>
		
    </licensePlate>

 <licensePlate filename="maps/map/licensePlates/licensePlates.i3d" node="1" type="SQUARISH" width="0.255" height="0.130">
        <font size="0.05" scaleX="0.95" scaleY="1"/>

        <variations>
            <variation> <!-- 3 region, 2 letter, 3 numbers -->
                <value node="0|0" character="E" posX="-0.066" posY="0.03" numerical="false" alphabetical="true" locked="false"/>
